{
  "private": true,
  "scripts": {
    "vercel:admin:env:pull": "./scripts/vercel/vercel-env-pull.sh",
    "ngrok": "npx ngrok start --all --config=ngrok.yml",
    "dev": "next dev",
    "dev:ngrok": "concurrently \"yarn dev\" \"yarn ngrok\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:sql": "for file in supabase/**/*.sql; do sql-formatter -l postgresql \"$file\" -o \"$file\"; done",
    "lint:sql": "yarn supabase:start:local && schemalint -c .schemalintrc.json",
    "lint:sql:ci": "supabase db lint --level error",
    "neat": "del .next && yarn format && yarn lint:fix && yarn format:sql && yarn lint:sql",
    "neat:dev": "yarn neat && yarn dev",
    "neat:build": "yarn neat && yarn build",
    "clean": "yarn neat && del node_modules && yarn cache clean && yarn",
    "clean:dev": "yarn clean && yarn dev",
    "clean:build": "yarn clean && yarn build",
    "supabase:start:local": "supabase start",
    "supabase:stop:local": "supabase stop",
    "supabase:studio:local": "open http://127.0.0.1:54323/",
    "supabase:list:local": "docker volume ls --filter label=com.supabase.cli.project=packages",
    "supabase:remove:local": "scripts/supabase/supabase-remove-local.sh",
    "supabase:types:local": "supabase gen types typescript --local > src/types/supabase.types.ts",
    "supabase:migration:new:local": "ts-node --esm scripts/supabase/supabase-migration-new-local.mjs",
    "supabase:migration:diff:local": "ts-node --esm scripts/supabase/supabase-migration-diff-local.mjs",
    "supabase:migration:squash:local": "yarn supabase:remove:local && supabase migration squash && yarn supabase:start:local",
    "supabase:reset:local": "supabase db reset",
    "supabase:test:local": "supabase db test",
    "supabase:lint:local": "supabase db lint",
    "supabase:admin:link:remote": "scripts/supabase/supabase-setup.sh",
    "supabase:admin:pull:remote": "yarn supabase:start:local && supabase db pull --schema auth,storage",
    "supabase:admin:push:remote": "supabase db push",
    "tools:tree": "scripts/general/print-project-tree.sh",
    "tools:generate:salt": "scripts/general/generate-salt.sh",
    "pre-commit": "yarn neat:build && yarn docs:lint",
    "docs:lint": "markdownlint \"docs/**/*.md\" \"**/*.md\" --ignore node_modules",
    "docs:lint:fix": "markdownlint \"docs/**/*.md\" \"**/*.md\" --ignore node_modules --fix",
    "docs:build": "mermaid -i docs/architecture/diagrams/*.mmd -o docs/architecture/diagrams",
    "prepare": "is-ci || run-s prepare:*",
    "prepare:all": "run-s prepare:permissions prepare:tools prepare:husky prepare:supabase",
    "prepare:permissions": "dotenv -- ts-node --esm scripts/general/set-permissions.mjs",
    "prepare:tools": "scripts/general/install-cli-tools.sh",
    "prepare:husky": "husky install",
    "prepare:supabase": "yarn supabase:start:local",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:api": "playwright test tests/api/**/*.spec.ts",
    "test:api:supabase": "playwright test tests/api/supabase/**/*.spec.ts",
    "test:api:gems": "playwright test tests/api/gems/*.api.spec.ts",
    "test:api:profiles": "playwright test tests/api/profiles/operations.api.spec.ts",
    "test:e2e": "playwright test tests/e2e",
    "test:email": "dotenv -e .env.local -- node scripts/general/test-email.mjs",
    "prisma:generate": "dotenv -e .env.local -- prisma generate",
    "prisma:db:pull": "dotenv -e .env.local -- prisma db pull --force",
    "prisma:migrate:dev:dryrun": "dotenv -e .env.local -- prisma migrate dev --create-only",
    "prisma:db:reset:safe": "dotenv -e .env.local -- prisma db reset",
    "precommit": "npm run lint:sql",
    "test:actions": "bash scripts/test-github-actions.sh",
    "test:actions:ci": "act pull_request -W .github/workflows/ci-cd.yml --secret-file .env.ci.local",
    "test:actions:ci:push": "act push -W .github/workflows/ci-cd.yml --secret-file .env.ci.local",
    "test:actions:local": "yarn test:actions"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@prisma/client": "5.22.0",
    "@radix-ui/react-accordion": "^1.2.10",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.1.1",
