    "eslint": "^9",
    "eslint-config-next": "15.0.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^8.0.0",
    "imapflow": "^1.0.169",
    "is-ci": "^3.0.1",
    "markdown-it": "^14.0.0",
    "markdown-it-mermaid": "^0.2.5",
    "markdownlint-cli": "^0.39.0",
    "ngrok": "^5.0.0-beta.2",
    "nodemailer": "^6.9.16",
    "npm-run-all": "^4.1.5",
    "postcss": "8.4.33",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "schemalint": "^2.0.5",
    "supabase": "^1.223.10",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3",
    "vercel": "^37.14.0"
  }
}
